{
  "name": "Findgrind",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:clean": "cd android && ./gradlew clean",
    "android:staging": "react-native run-android --mode=stagingDebug",
    "android:development": "react-native run-android --mode=developmentDebug",
    "android:production": "react-native run-android --mode=productionDebug",
    "android:stagingRelease": "react-native build-android --mode=stagingRelease",
    "android:developmentRelease": "react-native build-android --mode=developmentRelease",
    "android:productionRelease": "react-native build-android --mode=productionRelease",
    "android:productionBundle": "cd android && ./gradlew clean && ./gradlew bundleProductionRelease",
    "ios:pod": "cd ios && pod install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.0",
    "react-native-config": "1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
